{
    "name": "pizaini/inlislite",
    "description": "INLIS Lite merupakan perangkat lunak (software) aplikasi otomasi perpustakaan yang dibangun dan dikembangkan oleh Perpustakaan Nasional Republik Indonesia (Perpusnas) sejak tahun 2011. ",
    "keywords": ["inlislite", "inlislite perpusnas", "inlislite perpustakaan nasional republik indonesia", "inlis"],
    "homepage": "http://inlislite.perpusnas.go.id",
    "type": "project",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Perpustakaan Nasional Republik Indonesia",
            "email": "alvin_vna@yahoo.com",
            "homepage": "http://www.inlislite.perpusnas.go.id/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "2amigos/yii2-chartjs-widget": "~2.0",
        "2amigos/yii2-date-picker-widget" : "~1.0",
        "2amigos/yii2-file-upload-widget": "~1.0",
        "2amigos/yii2-highcharts-widget": "~1.0",
        "bower-asset/sweetalert" : "1.1.3",
        "codeplex/phpexcel": "1.7.8",
        "drsdre/yii2-wizardwidget": "1.1.*",
        "henryfinasmart/yii2-querybuilder": "1.0.*",
        "hscstudio/yii2-export": "1.0.0",
        "hscstudio/yii2-mimin": "1.1.*",
        "inlislite/yii2-inlislitegii": "dev-master",
        "kartik-v/yii2-editable": "1.7.*",
        "kartik-v/yii2-field-range": "1.3.*",
        "kartik-v/yii2-money": "1.2.*",
        "kartik-v/yii2-mpdf": "1.0.*",
        "kartik-v/yii2-popover-x": "1.3.*",
        "kartik-v/yii2-widget-colorinput": "1.0.*",
        "kartik-v/yii2-widget-growl": "1.1.*",
        "mdmsoft/yii2-admin": "~2.0",
        "moonlandsoft/yii2-phpexcel": "*",
        "newerton/yii2-jcrop": "dev-master",
        "open-ecommerce/yii2-backuprestore": "dev-master",
        "phpword/phpword": "dev-master",
        "picqer/php-barcode-generator": "^0.1.0",
        "psliwa/php-pdf": "1.2.*@dev",
        "unclead/yii2-multiple-input": "~1.0",
        "wbraganca/yii2-dynamicform": "2.0.*",
        "yiisoft/yii2": "2.0.7",
        "yiisoft/yii2-bootstrap": "2.0.*",
        "yiisoft/yii2-elasticsearch": "^2.0@dev",
        "yiisoft/yii2-httpclient": "^2.0@dev",
        "yiisoft/yii2-imagine": "~2.0.0",
        "yiisoft/yii2-jui": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "2.0.*",
        "zendframework/zend-barcode": "^2.6"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "fxp-asset": {
            "enabled": false,
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        },
        "process-timeout": 1800
    }
}
